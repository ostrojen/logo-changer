{
  "manifest_version": 2,
  "name": "Logo Changer",
  "version": "1.0",
  "description": "Change logo",
  "icons": {
    "48": "icon48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "run_at": "document_start",
      "js": [
        "content-script.js"
      ]
    }
  ]
}
